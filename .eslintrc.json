{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "no-unused-vars": "off",
    "implicit-arrow-linebreak": "off",
    "prefer-object-spread": "off",
    "consistent-return": "off",
    "no-shadow": "off",
    "jsx-a11y/label-has-associated-control": "off",
    "react/no-array-index-key": "off",
    "react/prop-types": "off"
  }
}
